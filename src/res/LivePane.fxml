<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_CENTER" fillWidth="false" spacing="20.0" type="VBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label styleClass="subtitle" text="Live flow preview" />
      <VBox fx:id="flowBox" alignment="CENTER_RIGHT" fillWidth="false" />
      <Button fx:id="resetButton" mnemonicParsing="false" prefWidth="150.0" text="Reset" />
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</fx:root>
