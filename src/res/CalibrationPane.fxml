<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainVbox" alignment="TOP_CENTER" spacing="5.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="jedrzejbronislaw.flowmeasure.controllers.CalibrationPaneController">
   <children>
      <ComboBox fx:id="flowmeterField" prefWidth="150.0" />
      <HBox alignment="TOP_CENTER" spacing="5.0">
         <children>
            <Button fx:id="startButton" mnemonicParsing="false" text="Start" />
            <Button fx:id="resetButton" mnemonicParsing="false" text="Reset" />
            <Button fx:id="stopButton" mnemonicParsing="false" text="Stop" />
         </children>
      </HBox>
      <Label fx:id="flowLabel" text="0" />
      <Button fx:id="setButton" mnemonicParsing="false" text="Set" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
